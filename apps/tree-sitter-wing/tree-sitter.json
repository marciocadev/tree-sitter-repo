{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "wing",
      "camelcase": "Wing",
      "title": "Wing",
      "scope": "source.wing",
      "file-types": [
        "w"
      ],
      "injection-regex": "^wing$",
      "class-name": "TreeSitterWing"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Wing grammar for tree-sitter",
    "authors": [
      {
        "name": "Marcio Cruz de Almeida",
        "email": "marciocadev@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-wing"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}