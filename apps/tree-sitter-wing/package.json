{
  "name": "tree-sitter-wing",
  "version": "0.1.0",
  "description": "Wing grammar for tree-sitter",
  "repository": "https://github.com/tree-sitter/tree-sitter-wing",
  "license": "MIT",
  "author": {
    "name": "Marcio Cruz de Almeida",
